{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": null,
  "roots": [
    "<rootDir>/test",
    "<rootDir>/src"
 ],
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "./coverage-e2e",
  "testTimeout": 90000,
  "collectCoverage": true,
  "collectCoverageFrom": [
    "<rootDir>/src/**/*.ts",
    "!<rootDir>/test/**/*.ts"
  ]
}
