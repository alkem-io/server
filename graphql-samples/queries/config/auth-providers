query
{
  configuration
  {
    authenticationProviders
    {
      name
      enabled
      config
      {
        __typename
      	... on AadConfig
        {
          msalConfig
          {
            auth
            {
              authority
              clientId
              redirectUri
            }
            cache
            {
              cacheLocation
              storeAuthStateInCookie
            }
          }
          apiConfig
          {
            resourceScope
          }
          authEnabled
      }
      ... on SimpleAuthProviderConfig {
        issuer
        tokenEndpoint
      }
    }


  }
}
}