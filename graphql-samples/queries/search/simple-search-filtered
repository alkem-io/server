query create($searchData: SearchInput!){
  search(searchData: $searchData) {
    score
    result {
      __typename
      ... on User {
        name
      }
      ... on UserGroup {
        name
      }
      ... on Organisation {
        name
      }
    }
  }
}

{
  "searchData":
  {
    "terms": ["Alisha", "members"],
    "typesFilter": ["user"]
  }
}