query search($searchData: SearchInput!) {
  search(searchData: $searchData) {
    contributorResults {
      id
      score
      terms
      type
      ... on SearchResultPost {
        space {
          nameID
        }
        challenge {
          nameID
        }
        opportunity {
          nameID
        }
        callout {
          nameID
        }
      }
    }
    journeyResults {
      id
      score
      terms
      type
      ... on SearchResultPost {
        space {
          nameID
        }
        challenge {
          nameID
        }
        opportunity {
          nameID
        }
        callout {
          nameID
        }
      }
    }
    contributionResults {
      id
      score
      terms
      type
      ... on SearchResultPost {
        space {
          nameID
        }
        challenge {
          nameID
        }
        opportunity {
          nameID
        }
        callout {
          nameID
        }
      }
    }
    groupResults {
      id
      score
      terms
      type
      ... on SearchResultPost {
        space {
          nameID
        }
        challenge {
          nameID
        }
        opportunity {
          nameID
        }
        callout {
          nameID
        }
      }
    }
  }
}





{
  "searchData":
  {
    "terms": ["post-name"]
  }
}