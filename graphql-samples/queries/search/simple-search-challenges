query search($searchData: SearchInput!){
  search(searchData: $searchData) {
    score
    result {
      id
      __typename
      ... on User {
        nameID
      }
      ... on UserGroup {
        name
      }
      ... on Organization {
        nameID
      }
      ... on Challenge {
        nameID
        hubID
      }
      ... on Opportunity {
        nameID
      }
    }
  }
}



{
  "searchData":
  {
    "terms": ["end", "poverty", "water"]
  }
}