query searchTagsets($searchData: SearchInput!){
  search(searchData: $searchData) {
    id
    score
    terms
    type
    ... on SearchResultHub {
      hub {
        nameID
      }
    }
    ... on SearchResultChallenge {
      challenge {
        nameID
      }
    }
    ... on SearchResultOpportunity {
      opportunity {
        nameID
      }
    }
    ... on SearchResultUser {
      user {
        nameID
      }
    }
    ... on SearchResultOrganization {
      organization {
        nameID
      }
    }
    ... on SearchResultCard {
      card {
        nameID
        profile {
          description
        }
      }
    }
  }
}

{
  "searchData":
  {
    "tagsetNames": ["Keywords"],
    "terms": ["twi", "Java", "architecture"]
  }
}