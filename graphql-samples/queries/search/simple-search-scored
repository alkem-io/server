query create($searchData: SearchInput!){
  search(searchData: $searchData) {
    score
    terms
    result {
      __typename
      ... on User {
        name
      }
      ... on UserGroup {
        name
      }
      ... on Organisation {
        name
      }
    }
  }
}

{
  "searchData":
  {
    "terms": ["Alisha", "Alisha", "Alisha", "Neil", "members"],
    "typesFilter": ["user", "group", "organisation"]
  }
}