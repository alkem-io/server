query create($searchData: SearchInput!){
  search(searchData: $searchData) {
    score
    terms
    type
    ... on SearchResultChallenge {
      challenge {
        nameID
      }
    }
    ... on SearchResultUser {
      user {
        nameID
      }
    }
    ... on SearchResultOrganization {
      organization {
        nameID
      }
    }
  }
}

{
  "searchData":
  {
    "tagsetNames": ["Keywords"],
    "terms": ["Alisha"]
  }
}