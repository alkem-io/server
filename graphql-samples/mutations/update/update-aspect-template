mutation updateAspectTemplate($aspectTemplateInput: UpdateAspectTemplateInput!) {
  updateAspectTemplate(aspectTemplateInput: $aspectTemplateInput) {
    profile {
      displayName
    }
  }
}

Variables:
{
  "aspectTemplateInput": {
      "ID": "uuid",
      "profile": {
        "displayName": "wow",
        "description": "wow this is great",
        "tags": ["Tag 1", "Tag 2"]
      },
      "type": "wow3",
      "defaultDescription": "this is what users will see"
  }
}