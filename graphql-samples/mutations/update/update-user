mutation UpdateUser($userData: UpdateUserInput!) {
  updateUser(userData: $userData) {
    displayName,
  	email
    profile {
      location {
        city
      }
    }
  }
}


query variables:
{
  "userData":
  {
    "ID": "uuid",
    "displayName": "Valentin",
    "profileData": {
      "ID": "uuid",
      "location": {
         "city": "Sofia"
      }
    }
  }
}
