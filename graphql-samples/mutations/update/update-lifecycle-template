mutation updateLifecycleTemplate($lifecycleTemplateInput: UpdateLifecycleTemplateInput!) {
  updateLifecycleTemplate(lifecycleTemplateInput: $lifecycleTemplateInput) {
    info {
      title
    }
  }
}

Variables:
{
  "lifecycleTemplateInput": {
      "ID": "uuid",
      "info": {
        "title": "wow",
        "description": "wow this is great",
        "tags": ["Tag 1", "Tag 2"]
      },
      "type": "challenge",
      "definition": "wow"
  }
}