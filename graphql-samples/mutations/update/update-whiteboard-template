mutation updateWhiteboardTemplate($whiteboardTemplateInput: UpdateWhiteboardTemplateInput!) {
  updateWhiteboardTemplate(whiteboardTemplateInput: $whiteboardTemplateInput) {
    profile {
      displayName
    }
  }
}

Variables:
{
  "whiteboardTemplateInput": {
      "ID": "uuid",
      "profile": {
        "displayName": "wow",
        "description": "wow this is great",
      },
      "content": "wow3"
  }
}