mutation updateInnovationFlowTemplate($innovationFlowTemplateInput: UpdateInnovationFlowTemplateInput!) {
  updateInnovationFlowTemplate(innovationFlowTemplateInput: $innovationFlowTemplateInput) {
    profile {
      displayName
    }
  }
}

Variables:
{
  "innovationFlowTemplateInput": {
      "ID": "uuid",
      "profile": {
        "displayName": "wow",
        "description": "wow this is great",
        "tags": ["Tag 1", "Tag 2"]
      },
      "type": "challenge",
      "states": "[]"
  }
}