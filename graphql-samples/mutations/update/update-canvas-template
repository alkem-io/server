mutation updateCanvasTemplate($canvasTemplateInput: UpdateCanvasTemplateInput!) {
  updateCanvasTemplate(canvasTemplateInput: $canvasTemplateInput) {
    profile {
      displayName
    }
  }
}

Variables:
{
  "canvasTemplateInput": {
      "ID": "uuid",
      "profile": {
        "displayName": "wow",
        "description": "wow this is great",
      },
      "value": "wow3"
  }
}