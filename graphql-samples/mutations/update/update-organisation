mutation updateOrganisation($organisationData: UpdateOrganisationInput!) {
  updateOrganisation(organisationData: $organisationData) {
    displayName,
    id
  }
}

query variables:
{
  "organisationData":
  {
    "ID": "uuid",
    "displayName": "Alkemio2",
    "profileData": {
      "description": "some description",
      "avatar": "http://someUri"
    },
    legalEntityName: "Alkemio Legal Name",
    "domain": "alkem.io",
    "website": "alkem.io",
    contactEmail: "contact@alkem.io"
  }
}

