mutation updateOrganisation($orgID: Float!, $organisationData: OrganisationInput!) {
  updateOrganisation(orgID: $orgID, organisationData: $organisationData) {
    name,
    id
  }
}

query variables:
{
  "orgID": 1,
  "organisationData":
  {
    "name": "Cherrytwist2",
    "profileData": {
      "description": "some description",
      "avatar": "http://someUri"
    }
  }
}

