mutation CreateOrganisation($organisationData: CreateOrganisationInput!) {
  createOrganisation(organisationData: $organisationData) {
    displayName
    nameID,
    id
    members
    {
     	nameID
    }
  }
}


query variables:
{
  "organisationData":
  {
    "nameID": "CherrytwistOrg",
    "displayName": "Stichting Cherrytwist"
  }
}