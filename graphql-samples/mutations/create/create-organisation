mutation CreateOrganisation($organisationData: CreateOrganisationInput!) {
  createOrganisation(organisationData: $organisationData) {
    displayName
    nameID,
    id
    members
    {
     	nameID
    }
  }
}


query variables:
{
  "organisationData":
  {
    "nameID": "AlkemioOrg",
    "displayName": "Stichting Alkemio",
    legalEntityName: "Alkemio Legal Name",
    "domain": "alkem.io",
    "website": "alkem.io",
    contactEmail: "contact@alkem.io"
  }
}
