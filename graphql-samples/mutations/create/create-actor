mutation createActor($actorData: CreateActorInput!) {
  createActor(actorData: $actorData) {
    id,
    name,
    description,
    value,
    impact
  }
}

query variables:
{
  "actorData": {
    "parentID": 1,
    "name": "developer1",
    "description": "Main architect for the solution",
    "value": "Ensuring a robust design",
    "impact": "Time allocated to work on the solution"
  }
}
