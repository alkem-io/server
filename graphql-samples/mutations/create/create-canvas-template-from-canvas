mutation createCanvasTemplate($canvasTemplateInput: CreateCanvasTemplateOnTemplatesSetInput!) {
  createCanvasTemplate(canvasTemplateInput: $canvasTemplateInput) {
    profile {
      displayName
    }
  }
}

Variables:
{
  "canvasTemplateInput": {
      "templatesSetID": "uuid",
      "profile": {
        "displayName": "wow",
        "description": "wow this is great"
      },
      "canvasID": "uuid"
  }
}