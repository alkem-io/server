mutation CreateProject($projectData: CreateProjectInput!) {
  createProject(projectData: $projectData) {
    name,
    description
  }
}


query variables:
{
  "projectData":
  {
    "parentID": 1,
    "name": "firstProject",
    "nameID": "tech-stack",
    "description": "How do I handle scalability?"
  }
}