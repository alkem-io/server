mutation CreateProject($projectData: CreateProjectInput!, $opportunityID: Float!) {
  createProject(projectData: $projectData, opportunityID: $opportunityID) {
    name,
    description,
    state
  }
}


query variables:
{
  "opportunityID": 1,
  "projectData":
  {
    "name": "TechStack2",
    "textID": "tech-stack",
    "description": "How do I handle scalability?",
    "state": "new"
  }
}