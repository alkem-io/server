mutation createGroupOnCommunity($groupData: CreateUserGroupInput!) {
  createGroupOnCommunity(groupData: $groupData) {
    name,
    id
    members {
      name
    }
  }
}

Variables:
{
  "groupData":
  {
      "name": "Valentin",
      "parentID": 1,
      "profileData": {
        "description": "some description",
        "avatar": "http://someUri"
      }
  }
}