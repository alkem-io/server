mutation CreateAspect($aspectData: CreateAspectInput!) {
  createAspect(aspectData: $aspectData) {
    title,
    framing,
    explanation
  }
}


query variables:
{
  "aspectData":
  {
    "contextID": "uuid",
    "title": "TechStack2",
    "framing": "How do I handle scalability?",
    "explanation": "Deployment is via a microservices architecture"
  }
}