mutation createCommunityGuidelineTemplate($communityGuidelineTemplateInput: CreateCommunityGuidelinesTemplateOnTemplatesSetInput!) {
  createCommunityGuidelineTemplate(communityGuidelineTemplateInput: $communityGuidelineTemplateInput) {
    profile {
      displayName
    }
  }
}

Variables:
{
  "communityGuidelineTemplateInput": {
      "templatesSetID": "uuid",
      "profile": {
        "displayName": "Community Guidelines Example",
        "description": "Dos and don'ts",
        "tags": ["Tag 1", "Tag 2"]
      },
  }
}