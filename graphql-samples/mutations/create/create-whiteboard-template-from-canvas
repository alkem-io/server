mutation createWhiteboardTemplate($whiteboardTemplateInput: CreateWhiteboardTemplateOnTemplatesSetInput!) {
  createWhiteboardTemplate(whiteboardTemplateInput: $whiteboardTemplateInput) {
    profile {
      displayName
    }
  }
}

Variables:
{
  "whiteboardTemplateInput": {
      "templatesSetID": "uuid",
      "profile": {
        "displayName": "wow",
        "description": "wow this is great"
      },
      "canvasID": "uuid"
  }
}