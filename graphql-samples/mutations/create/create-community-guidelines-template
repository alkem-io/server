mutation createCommunityGuidelineTemplate($communityGuidelineTemplateInput: CreateCommunityGuidelinesTemplateOnTemplatesSetInput!) {
  createCommunityGuidelineTemplate(communityGuidelineTemplateInput: $communityGuidelineTemplateInput) {
    profile {
      displayName
      description
    }
    guidelines {
      profile {
        description
        displayName
      }
    }
  }
}

Variables:
{
  "communityGuidelineTemplateInput": {
      "templatesSetID": "uuid",
      "profile": {
        "displayName": "Community Guidelines template Example",
        "description": "Template description"
      },
      communityGuidelines:{
        profile:{
          displayName:"Guidelines name",
          description:"do not be rude"
        }
      }
  }
}