mutation createActorGroup($actorGroupData: CreateActorGroupInput!) {
  createActorGroup(actorGroupData: $actorGroupData) {
    title,
    framing,
    explanation
  }
}


query variables:
{
  "actorGroupData":
  {
    "ecosystemModelID": "uuid",
    "name": "TechStack2",
    "description": "How do I handle scalability?",
  }
}