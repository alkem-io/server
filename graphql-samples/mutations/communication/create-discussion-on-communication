mutation createDiscussion($createData: CommunicationCreateDiscussionInput!) {
  createDiscussion(createData: $createData) {
    profile {
      displayName,
      description
    },
    category
  }
}

Variables:
{
  "createData": {
    "communicationID": "f5478869-958e-429c-b146-c0dda0a9eb38",
    "profile": {
      "displayName":  "some new title",
      "description": "text"
    },
    "category": "PLATFORM_FUNCTIONALITIES"
  }
}