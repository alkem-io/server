{
  "name": "alkemio-server",
  "version": "0.83.1",
  "description": "Alkemio server, responsible for managing the shared Alkemio platform",
  "author": "Alkemio Foundation",
  "private": false,
  "license": "EUPL-1.2",
  "main": "src/main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alkem-io/server.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start-nodemon": "nodemon -w src --ext ts --exec ts-node src/main.ts",
    "start-nodemon-local": "nodemon -w src --ext ts --exec ts-node -r dotenv/config src/main.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:services": "docker compose -f quickstart-services.yml --env-file .env.docker up --build --force-recreate",
    "start:services:ssi": "docker compose -f quickstart-services.yml -f quickstart-wallet-manager.yml --env-file .env.docker up --build --force-recreate",
    "start:services:kratos:debug": "docker compose -f quickstart-services-kratos-debug.yml --env-file .env.docker up --build --force-recreate",
    "start:services:ai": "docker compose -f quickstart-services-ai.yml --env-file .env.docker up --build --force-recreate",
    "start:services:ai:debug": "docker compose -f quickstart-services-ai-debug.yml --env-file .env.docker up --build --force-recreate",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "lint:prod": "tsc --noEmit && cross-env NODE_ENV=production eslint src/**/*.ts{,x}",
    "lint:fix": "tsc --noEmit && eslint src/**/*.ts{,x} --fix",
    "test": "jest --config ./test/config/jest.config.js",
    "test:watch": "jest --watch --config ./test/config/jest.config.js",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:ci": "jest --forceExit --config ./test/config/jest.config.ci.js && cat ./coverage-ci/lcov.info | coveralls",
    "test:ci:no:coverage": "jest --forceExit --config ./test/config/jest.config.ci.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource src/config/migration.create.config.ts",
    "typeorm-no-entities": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource src/config/migration.config.ts",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:run": "npm run typeorm-no-entities migration:run",
    "migration:revert": "npm run typeorm-no-entities migration:revert",
    "migration:show": "npm run typeorm migration:show",
    "migration:validate": "./.scripts/migrations/run_validate_migration.sh"
  },
  "overrides": {
    "fs-capacitor": "6.2.0",
    "jsonwebtoken": "9.0.0",
    "@nestjs/schematics": {
      "typescript": "5.3.3"
    }
  },
  "dependencies": {
    "@alkemio/matrix-adapter-lib": "^0.4.1",
    "@alkemio/notifications-lib": "^0.9.6",
    "@apollo/server": "^4.10.2",
    "@elastic/elasticsearch": "^8.4.0",
    "@golevelup/nestjs-rabbitmq": "^5.3.0",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.7",
    "@nestjs/config": "^3.2.1",
    "@nestjs/core": "^10.3.7",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.7",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/typeorm": "10.0.2",
    "@ory/kratos-client": "^0.11.1",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/lodash": "^4.14.197",
    "ajv": "^8.11.0",
    "amqp-connection-manager": "^3.7.0",
    "amqplib": "^0.8.0",
    "cache-manager-redis-store": "^2.0.0",
    "chromadb": "^1.8.1",
    "cids": "^1.1.9",
    "class-transformer": "^0.4.0",
    "class-validator": "0.14.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dataloader": "^2.2.2",
    "file-type": "^16.5.3",
    "graphql": "^16.6.0",
    "graphql-amqp-subscriptions": "^1.2.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^13.0.0",
    "graphql-ws": "^5.6.2",
    "helmet": "^4.6.0",
    "image-size": "^1.0.0",
    "jwks-rsa": "^2.0.4",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "logform": "^2.3.2",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "mysql2": "^2.3.3",
    "nest-winston": "^1.9.4",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.0",
    "redis": "^3.1.2",
    "redlock": "^5.0.0-beta.2",
    "replace-special-characters": "^1.2.5",
    "rxjs": "^7.3.0",
    "socket.io": "^4.7.5",
    "typeorm": "0.3.11",
    "winston": "^3.3.3",
    "xstate": "^4.37.2",
    "yaml": "^2.0.0-7"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.7",
    "@types/amqplib": "^0.8.2",
    "@types/cache-manager": "^3.4.2",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/graphql-upload": "^8.0.11",
    "@types/jest": "^26.0.24",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.4.6",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "coveralls": "^3.1.1",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.8",
    "jest": "^29.3.1",
    "jest-mock": "^28.1.1",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "supertest": "^6.1.4",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "5.3.3"
  },
  "_moduleAliases": {
    "@src": "./dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10"
  },
  "volta": {
    "node": "20.13.1"
  }
}
