{
  "name": "alkemio-server",
  "version": "0.11.9",
  "description": "Alkemio server, responsible for managing the shared Alkemio platform",
  "author": "Cherrytwist Foundation",
  "private": false,
  "license": "EUPL-1.2",
  "main": "src/main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alkem-io/server.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start-nodemon": "nodemon -w src --ext ts --exec ts-node src/main.ts",
    "start-nodemon-local": "nodemon -w src --ext ts --exec ts-node -r dotenv/config src/main.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "lint:prod": "tsc --noEmit && cross-env NODE_ENV=production eslint src/**/*.ts{,x}",
    "lint:fix": "tsc --noEmit && eslint src/**/*.ts{,x} --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:ci": "jest --forceExit --runInBand --config ./test/config/jest.config.ci.js && cat ./coverage-ci/lcov.info | coveralls",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/typeorm.cli.config.ts",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:show": "npm run typeorm migration:show"
  },
  "dependencies": {
    "@jolocom/sdk": "^1.2.0",
    "@jolocom/sdk-storage-typeorm": "^4.2.0",
    "@jolocom/vaulted-key-provider": "^0.8.0",
    "@nestjs/axios": "^0.0.1",
    "@nestjs/common": "^8.0.5",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.5",
    "@nestjs/graphql": "^8.0.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.0.5",
    "@nestjs/typeorm": "^8.0.1",
    "@ory/kratos-client": "^0.6.3-alpha.1",
    "@types/crypto-js": "^4.0.1",
    "@types/express-session": "^1.17.3",
    "apollo-server-express": "^2.22.2",
    "axios": "^0.21.1",
    "cd": "^0.3.3",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.12.2",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.0.0",
    "express-session": "^1.17.1",
    "graphql": "^15.5.0",
    "graphql-tools": "^6.2.6",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^11.0.0",
    "helmet": "^4.4.1",
    "image-size": "^0.9.7",
    "ipfs-http-client": "^49.0.4",
    "jolocom-lib": "^5.2.1",
    "js-yaml": "^4.1.0",
    "jwks-rsa": "^2.0.3",
    "matrix-js-sdk": "^9.11.0",
    "module-alias": "^2.2.2",
    "mysql": "^2.18.1",
    "nest-winston": "^1.4.0",
    "nodemon": "^2.0.7",
    "normalizer": "^1.2.7",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "replace-special-characters": "^1.2.5",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.35",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-elasticsearch": "^0.15.8",
    "xstate": "^4.23.1",
    "yaml": "2.0.0-5"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.15",
    "@types/cache-manager": "^3.4.0",
    "@types/express": "^4.17.11",
    "@types/graphql-upload": "^8.0.4",
    "@types/jest": "^26.0.22",
    "@types/js-yaml": "^4.0.0",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^13.13.48",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "coveralls": "^3.1.0",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.4",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "_moduleAliases": {
    "@src": "./dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=14.17.3"
  }
}
