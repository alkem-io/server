{
  "name": "alkemio-server",
  "version": "0.66.1",
  "description": "Alkemio server, responsible for managing the shared Alkemio platform",
  "author": "Alkemio Foundation",
  "private": false,
  "license": "EUPL-1.2",
  "main": "src/main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alkem-io/server.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start-nodemon": "nodemon -w src --ext ts --exec ts-node src/main.ts",
    "start-nodemon-local": "nodemon -w src --ext ts --exec ts-node -r dotenv/config src/main.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:services": "docker-compose -f quickstart-services.yml --env-file .env.docker up --build --force-recreate",
    "start:services:kratos:debug": "docker-compose -f quickstart-services-kratos-debug.yml --env-file .env.docker up --build --force-recreate",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "lint:prod": "tsc --noEmit && cross-env NODE_ENV=production eslint src/**/*.ts{,x}",
    "lint:fix": "tsc --noEmit && eslint src/**/*.ts{,x} --fix",
    "test": "jest --config ./test/config/jest.config.js",
    "test:watch": "jest --watch --config ./test/config/jest.config.js",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:ci": "jest --forceExit --config ./test/config/jest.config.ci.js && cat ./coverage-ci/lcov.info | coveralls",
    "test:ci:no:coverage": "jest --forceExit --config ./test/config/jest.config.ci.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource src/config/migration.create.config.ts",
    "typeorm-no-entities": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource src/config/migration.config.ts",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:run": "npm run typeorm-no-entities migration:run",
    "migration:revert": "npm run typeorm-no-entities migration:revert",
    "migration:show": "npm run typeorm migration:show",
    "migration:validate": "./.scripts/migrations/run_validate_migration.sh"
  },
  "overrides": {
    "fs-capacitor": "6.2.0",
    "jsonwebtoken": "9.0.0",
    "@nestjs/schematics": {
      "typescript": "4.7.4"
    }
  },
  "dependencies": {
    "@alkemio/matrix-adapter-lib": "^0.3.6",
    "@alkemio/notifications-lib": "^0.6.0",
    "@apollo/server": "^4.6.0",
    "@elastic/elasticsearch": "^8.4.0",
    "@nestjs/apollo": "^10.0.6",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^9.4.3",
    "@nestjs/graphql": "^10.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^9.4.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/typeorm": "9.0.1",
    "@ory/kratos-client": "^0.11.1",
    "@socket.io/redis-adapter": "^8.2.1",
    "@types/lodash": "^4.14.197",
    "ajv": "^8.11.0",
    "amqp-connection-manager": "^3.7.0",
    "amqplib": "^0.8.0",
    "cache-manager": "^3.4.4",
    "cache-manager-redis-store": "^2.0.0",
    "cids": "^1.1.9",
    "class-transformer": "^0.4.0",
    "class-validator": "0.14.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dataloader": "^2.2.2",
    "file-type": "^16.5.3",
    "graphql": "^16.6.0",
    "graphql-amqp-subscriptions": "^1.2.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^13.0.0",
    "graphql-ws": "^5.6.2",
    "helmet": "^4.6.0",
    "image-size": "^1.0.0",
    "jwks-rsa": "^2.0.4",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "logform": "^2.3.2",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "mysql": "^2.18.1",
    "nest-winston": "^1.5.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.0",
    "redis": "^3.1.2",
    "redlock": "^5.0.0-beta.2",
    "replace-special-characters": "^1.2.5",
    "rxjs": "^7.3.0",
    "socket.io": "^4.7.1",
    "typeorm": "0.3.11",
    "winston": "^3.3.3",
    "xstate": "^4.37.2",
    "yaml": "^2.0.0-7"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.0",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^9.4.0",
    "@types/amqplib": "^0.8.2",
    "@types/cache-manager": "^3.4.2",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/graphql-upload": "^8.0.11",
    "@types/jest": "^26.0.24",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.4.6",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "coveralls": "^3.1.1",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.8",
    "jest": "^29.3.1",
    "jest-mock": "^28.1.1",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "supertest": "^6.1.4",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4"
  },
  "_moduleAliases": {
    "@src": "./dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=16.15.0",
    "npm": ">=8.5.5"
  }
}
