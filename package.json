{
  "name": "alkemio-server",
  "version": "0.128.3",
  "description": "Alkemio server, responsible for managing the shared Alkemio platform",
  "author": "Alkemio Foundation",
  "private": false,
  "license": "EUPL-1.2",
  "main": "src/main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alkem-io/server.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "cp alkemio.yml dist/alkemio.yml",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start-nodemon": "nodemon -w src --ext ts --exec ts-node src/main.ts",
    "start-nodemon-local": "nodemon -w src --ext ts --exec ts-node -r dotenv/config src/main.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "start:services": "docker compose -f quickstart-services.yml --env-file .env.docker up --build --force-recreate -d",
    "start:services:ssi": "docker compose -f quickstart-services.yml -f quickstart-wallet-manager.yml --env-file .env.docker up --build --force-recreate",
    "start:services:kratos:debug": "docker compose -f quickstart-services-kratos-debug.yml --env-file .env.docker up --build --force-recreate",
    "start:services:ai": "docker compose -f quickstart-services-ai.yml --env-file .env.docker up --build --force-recreate",
    "start:services:ai:debug": "docker compose -f quickstart-services-ai-debug.yml --env-file .env.docker up --build --force-recreate",
    "lint": "tsc --noEmit && eslint src/**/*.ts --no-warn-ignored",
    "lint:prod": "tsc --noEmit && cross-env NODE_ENV=production eslint src/**/*.ts --no-warn-ignored",
    "lint:fix": "tsc --noEmit && eslint src/**/*.ts --fix --no-warn-ignored",
    "test": "jest --config ./test/config/jest.config.js",
    "test:watch": "jest --watch --config ./test/config/jest.config.js",
    "test:cov": "jest --coverage --config ./test/config/jest.config.js",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:ci": "jest --forceExit --config ./test/config/jest.config.ci.js && cat ./coverage-ci/lcov.info",
    "test:ci:no:coverage": "jest --forceExit --config ./test/config/jest.config.ci.nocov.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource src/config/migration.create.config.ts",
    "typeorm-no-entities": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource src/config/migration.config.ts",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "migration:generate": "pnpm run typeorm migration:generate",
    "migration:run": "pnpm run typeorm-no-entities migration:run",
    "migration:revert": "pnpm run typeorm-no-entities migration:revert",
    "migration:show": "pnpm run typeorm migration:show",
    "migration:validate": "./.scripts/migrations/run_validate_migration.sh",
    "circular-dependencies": "madge --circular dist",
    "db:recreate": "ts-node .scripts/tests/recreate-db.ts",
    "db:reset": "pnpm run db:recreate && pnpm run migration:run",
    "schema:print": "ts-node -r tsconfig-paths/register src/tools/schema/print-schema.ts schema.graphql",
    "schema:sort": "ts-node -r tsconfig-paths/register src/tools/schema/sort-sdl.ts schema.graphql",
    "schema:diff": "ts-node -r tsconfig-paths/register src/tools/schema/diff-schema.ts --old tmp/prev.schema.graphql --new schema.graphql --out change-report.json --deprecations deprecations.json",
    "schema:validate": "ts-node -r tsconfig-paths/register src/tools/schema/validate-artifacts.ts change-report.json deprecations.json"
  },
  "overrides": {
    "fs-capacitor": "6.2.0",
    "jsonwebtoken": "9.0.0",
    "tough-cookie": "4.1.3",
    "@nestjs/schematics": {
      "typescript": "5.3.3"
    }
  },
  "dependencies": {
    "@alkemio/notifications-lib": "0.12.2",
    "@alkemio/matrix-adapter-lib": "^0.6.2",
    "@apollo/server": "^4.10.4",
    "@elastic/elasticsearch": "^8.12.2",
    "@golevelup/nestjs-rabbitmq": "^5.3.0",
    "@hedger/nestjs-encryption": "^0.1.5",
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.10",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/typeorm": "10.0.2",
    "@ory/kratos-client": "^1.2.0",
    "@tiptap/core": "^3.6.1",
    "@tiptap/extension-highlight": "^3.2.2",
    "@tiptap/extension-image": "^3.2.2",
    "@tiptap/starter-kit": "^3.2.0",
    "@tiptap/static-renderer": "^3.2.0",
    "@tiptap/y-tiptap": "^3.0.0",
    "ajv": "^8.17.1",
    "amqplib": "0.10.3",
    "axios": "^1.12.2",
    "body-parser": "1.20.3",
    "cache-manager-redis-store": "^2.0.0",
    "chromadb": "1.8.1",
    "cids": "^1.1.9",
    "class-transformer": "^0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dataloader": "^2.2.3",
    "dotenv": "16.4.5",
    "elastic-apm-node": "^4.11.0",
    "graphql": "^16.9.0",
    "graphql-amqp-subscriptions": "^2.0.0",
    "graphql-helix": "^1.13.0",
    "graphql-http": "^1.22.3",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^13.0.0",
    "graphql-ws": "^5.6.2",
    "helmet": "^4.6.0",
    "i18n-iso-countries": "^7.14.0",
    "image-size": "^1.1.1",
    "jwks-rsa": "^2.0.4",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "logform": "^2.6.1",
    "markdown-it": "^14.1.0",
    "module-alias": "^2.2.3",
    "mysql2": "^3.10.3",
    "nest-winston": "^1.9.7",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "path-to-regexp": "^8.2.0",
    "pg": "^8.13.1",
    "prosemirror-markdown": "^1.13.2",
    "prosemirror-model": "^1.25.3",
    "redis": "3.1.2",
    "reflect-metadata": "^0.2.2",
    "replace-special-characters": "^1.2.7",
    "rxjs": "^7.8.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typeorm": "0.3.13",
    "winston": "^3.13.1",
    "xstate": "5.18.0",
    "yaml": "^2.4.5",
    "yjs": "^13.6.27"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.39.1",
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.3.10",
    "@types/amqplib": "^0.10.5",
    "@types/body-parser": "^1.19.6",
    "@types/cache-manager": "^4.0.6",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/graphql-upload": "^8.0.11",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.14",
    "@types/markdown-it": "^14.1.2",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.14.10",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "coveralls": "^3.1.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "16.5.0",
    "husky": "^4.3.8",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.2",
    "typescript": "5.3.3"
  },
  "_moduleAliases": {
    "@src": "./dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=20.9.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@10.17.1",
  "volta": {
    "node": "20.15.1"
  }
}
