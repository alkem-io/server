{
  "name": "alkemio-server",
  "version": "0.28.2",
  "description": "Alkemio server, responsible for managing the shared Alkemio platform",
  "author": "Alkemio Foundation",
  "private": false,
  "license": "EUPL-1.2",
  "main": "src/main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alkem-io/server.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start-nodemon": "nodemon -w src --ext ts --exec ts-node src/main.ts",
    "start-nodemon-local": "nodemon -w src --ext ts --exec ts-node -r dotenv/config src/main.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:services": "docker-compose -f quickstart-services.yml --env-file .env.docker up --build --force-recreate",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "lint:prod": "tsc --noEmit && cross-env NODE_ENV=production eslint src/**/*.ts{,x}",
    "lint:fix": "tsc --noEmit && eslint src/**/*.ts{,x} --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:ci": "jest --forceExit --runInBand --config ./test/config/jest.config.ci.js && cat ./coverage-ci/lcov.info | coveralls",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/typeorm.cli.config.ts",
    "typeorm-no-entities": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/typeorm.cli.config.run.ts",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm-no-entities migration:run",
    "migration:revert": "npm run typeorm-no-entities migration:revert",
    "migration:show": "npm run typeorm migration:show"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.6",
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "^8.0.5",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.5",
    "@nestjs/graphql": "^10.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.1.2",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.0.5",
    "@nestjs/typeorm": "^8.0.1",
    "@ory/kratos-client": "^0.6.3-alpha.1",
    "@types/crypto-js": "^4.0.1",
    "@types/express-session": "^1.17.3",
    "@types/matrix-js-sdk": "^11.0.1",
    "amqp-connection-manager": "^3.7.0",
    "amqplib": "^0.8.0",
    "apollo-server-express": "^3.6.3",
    "cache-manager": "^3.4.4",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "graphql": "^15.8.0",
    "graphql-amqp-subscriptions": "^1.2.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^13.0.0",
    "graphql-ws": "^5.6.2",
    "helmet": "^4.6.0",
    "image-size": "^1.0.0",
    "ipfs-http-client": "^51.0.1",
    "jwks-rsa": "^2.0.4",
    "jwt-decode": "^3.1.2",
    "logform": "^2.3.2",
    "matrix-js-sdk": "^15.2.0",
    "module-alias": "^2.2.2",
    "mysql": "^2.18.1",
    "nest-winston": "^1.5.0",
    "nodemon": "^2.0.12",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "replace-special-characters": "^1.2.5",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "typeorm": "^0.2.43",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-elasticsearch": "^0.15.8",
    "xstate": "^4.26.1",
    "yaml": "^2.0.0-7"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.0",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.5",
    "@types/amqplib": "^0.8.2",
    "@types/cache-manager": "^3.4.2",
    "@types/crypto-js": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/graphql-upload": "^8.0.11",
    "@types/jest": "^26.0.24",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.4.6",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "coveralls": "^3.1.1",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.8",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "_moduleAliases": {
    "@src": "./dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=16.15.0",
    "npm": ">=8.5.5"
  }
}
